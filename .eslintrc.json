{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es2022": true
  },
  "rules": {
    "import/extensions": ["error", "ignorePackages"],
    "no-console": "off",
    "import/prefer-default-export": "off"
  }
} 